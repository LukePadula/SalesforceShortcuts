{
  "manifest_version": 3,
  "name": "Salesforce Shortcuts",
  "version": "1.0",
  "description": "A Salesforce shortcut extension for admins",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "storage", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"]
}
